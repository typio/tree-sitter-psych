{
  "name": "tree-sitter-psych",
  "devDependencies": {
    "@types/bun": "latest",
    "prebuildify": "^6.0.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "tree-sitter-cli": "^0.22.5",
    "node-addon-api": "^7.1.0",
    "node-gyp-build": "^4.8.0"
  },
  "scripts": {
    "install": "node-gyp-build",
    "prebuildify": "prebuildify --napi --strip"
